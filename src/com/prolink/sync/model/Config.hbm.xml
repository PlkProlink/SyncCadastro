<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- Tiago Dias
-->
<hibernate-mapping package="com.prolink.sync.model">
  <class name="ConfigBean" table="cad_upgrade_config">
    <id name="CODIGO" column="CODIGO" type="int">
    	<generator class="identity" />
    </id>
    <property name="SEGUNDO" length="100"/>
    <property name="MINUTO" length="100"/>
    <property name="HORA" length="100"/>
    <property name="DIA_DO_MES" length="100"/>
    <property name="MES" length="100"/>
    <property name="DIA_DA_SEMANA" length="200"/>
  </class>
</hibernate-mapping>