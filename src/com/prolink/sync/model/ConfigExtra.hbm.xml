<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- Tiago Dias
-->
<hibernate-mapping package="com.prolink.sync.model">
	<class name="ConfExtraBean" table="PLANILHA_CONFIG">
		<id name="CODIGO" column="CODIGO" type="int">
			<generator class="identity" />
		</id>
		<property name="PLANILHA_NOME" length="100"/>
		<property name="PLANILHA_LOCALIZACAO" length="100"/>
		<property name="DIRETORIO_TEMP"  length="100"/>
	</class>
</hibernate-mapping>