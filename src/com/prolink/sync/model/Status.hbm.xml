<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- Tiago Dias
-->
<hibernate-mapping package="com.prolink.sync.model">
	<class name="Status" table="cad_upgrade_status">
	 	<id name="COD" column="cod" type="int">
	 		<generator class="identity" />
	 	</id>
	 	<property name="DATA" column="data" type="java.util.Date" />
	 	<property name="TEMPOLEITURA" column="tempo_leitura" type="long" />
	 	<property name="TEMPOGRAVACAO" column="tempo_gravacao" type="long" />
	 	<property name="DETALHES" column="detalhes"/>
	 </class>
</hibernate-mapping>