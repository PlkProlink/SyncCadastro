<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="model">
  <class name="ConfigBean" table="CONFIG">
    <id name="CODIGO" column="CODIGO" type="int">
    	<generator class="native" />
    </id>
    <property name="SEGUNDO" length="100"/>
    <property name="MINUTO" length="100"/>
    <property name="HORA" length="100"/>
    <property name="DIA_DO_MES" length="100"/>
    <property name="MES" length="100"/>
    <property name="DIA_DA_SEMANA" length="200"/>
  </class>
</hibernate-mapping>